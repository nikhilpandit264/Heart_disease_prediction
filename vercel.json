{
  "version": 2,
  "builds": [
    { "src": "app.py", "use": "@vercel/python" },
    { "src": "landing.html", "use": "@vercel/static" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/$", "dest": "landing.html" },
    { "src": "/predict", "dest": "app.py" },
    { "src": "/.*", "dest": "app.py" }
  ]
}
